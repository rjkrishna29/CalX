<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.CalX" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:statusBarColor">#F6F6F6</item>
    </style>
    <style name="fffffddddd" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:statusBarColor">#F6F6F6</item>
    </style>
    <style name="txtBg">
        <item name="android:background">#DEDCDC</item>
    </style>
    <style name="signButton">
        <item name="android:backgroundTint">#FFFFFF</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <color name="textBackground">#DEDCDC</color>
    <color name="settinglayout">#FFFFFF</color>
    <style name="disableButton">
        <item name="android:textColor">#CCCCCC</item>
    </style>
    <style name="enableButton">
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="settings">
        <item name="cornerRadius">18dp</item>
        <item name="android:background">@drawable/rounded_corners</item>
        <item name="background">@color/settinglayout</item>
    </style>
    <style name="Base.Theme.CalX.Fullscreen" parent="Base.Theme.CalX">
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:fitsSystemWindows">true</item>
        <item name="android:background">#F6F6F6</item>
    </style>
    <style name="aboutSettings">
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="menu_share_icon">
        <item name="android:drawableLeft">@drawable/baseline_share_23</item>
    </style>
    <style name="CustomBackgroundStyle">
        <item name="android:background">#F6F6F6</item>
    </style>

    <style name="CustomBackgroundStyleSettings">
        <item name="android:background">#E7E7E7</item>
    </style>
    <style name="changeDigit_tv1">
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="changeDigit_tv2">
        <item name="android:textColor">#C7C7C7</item>
    </style>
    <style name="Theme.CalX" parent="Base.Theme.CalX" />
    <style name="Text">
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="Text2">
        <item name="android:textColor">#706E6E</item>
    </style>
    <style name="operatorButton" parent="commonButton">
        <item name="android:backgroundTint">@color/btnBackground2</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="view">
        <item name="background">@color/white</item>
    </style>
    <style name="view2">
        <item name="backgroundColor">@color/black</item>
    </style>
        <style name="digitButton" parent="commonButton">
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="digitButton2" parent="commonButton">
        <item name="android:textColor">@color/black</item>
        <item name="android:backgroundTint">#EBEAEA</item>
    </style>
    <style name="backButton" parent="digitButton">
        <item name="android:layout_width">40dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="cornerRadius">40dp</item>
        <item name="android:textSize">5sp</item>
    </style>
    
    <style name="moreButton">
        <item name="backgroundTint">@color/white</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="btnBack" parent = "digitButton2">
        <item name="android:drawableLeft">@drawable/baseline_backspace_24</item>
    </style>
    <style name="backSpace" >
        <item name="android:drawableLeft">@drawable/baseline_backspace_24</item>
        <item name="android:backgroundTint">#F6F6F6</item>
    </style>
    <style name="backSpace2" >
        <item name="android:drawableLeft">@drawable/baseline_backspace_24</item>
        <item name="android:backgroundTint">#FFFFFF</item>
    </style>

    <style name="commonButton" parent="Widget.MaterialComponents.ExtendedFloatingActionButton">
        <item name="android:layout_width">65dp</item>
        <item name="android:layout_height">65dp</item>
        <item name="backgroundTint">@color/white</item>
        <item name="cornerRadius">36dp</item>
        <item name="android:textSize">32dp</item>
        <item name="android:layout_margin">12dp</item>

    </style>
<color name="strikeLine">#E7E7E7</color>
</resources>